[database]
provider = postgres
dataSource = user=testuser password=testpass dbname=testdb sslmode=disable

[settings]
; the alphabet is used to create unique codes. Once an app is in production, it can be dangerous to change
; this, so choose wisely.
;alphabet = 0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ # standard base62
alphabet = 23456789abcdefghijkmnpqrstuvwxyzABCDEFGHJKLMNPQRSTUVWXYZ # without ambiguous characters
